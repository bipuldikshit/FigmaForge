# Figma API Configuration
FIGMA_TOKEN=your_figma_personal_access_token_here

# MCP Server Configuration
MCP_SERVER_PORT=8080
MCP_SERVER_HOST=127.0.0.1

# Output Paths
ANGULAR_OUTPUT_PATH=./src/app/components
ASSETS_OUTPUT_PATH=./src/assets/figma

# Generator Options
DEFAULT_STYLE_FORMAT=scss  # scss or tailwind
INLINE_ASSET_SIZE_LIMIT=2048  # bytes, assets smaller than this will be inlined

# Cache Configuration
CACHE_DIR=./.figma-cache
ENABLE_CACHE=true
